{
    "name": "outerweb/filament-link-picker",
    "description": "This package adds a field to pick a link from your defined routes or external links.",
    "homepage": "https://github.com/outer-web/filament-link-picker",
    "license": "MIT",
    "authors": [
        {
            "name": "Outerweb",
            "email": "info@outerweb.be"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^3.2",
        "laravel/framework": "^10.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "Outerweb\\FilamentLinkPicker\\": "src/"
        },
        "files": [
            "src\\Helpers\\helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Outerweb\\FilamentLinkPicker\\FilamentLinkPickerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
